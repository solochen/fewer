<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv_winner_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="15dp"
        android:layout_marginTop="50dp"
        android:background="#cccfff"
        android:textColor="#fff"
        android:layout_gravity="center_horizontal"
        android:text="@string/label_lib_winner_number"/>

    <com.yilan.lib.playerlib.widget.CustomRecycleView
        android:id="@+id/lib_player_winner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:background="@color/lib_translate_color"
        android:cacheColorHint="@color/lib_translate_color"
        android:divider="@color/lib_translate_color"
        android:fadingEdgeLength="10dp"
        android:listSelector="@color/lib_translate_color"
        android:requiresFadingEdge="vertical" />

</FrameLayout>